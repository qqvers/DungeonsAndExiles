{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ClientRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "post:/api/players/*/monsters/*",
        "Period": "10s",
        "Limit": 1
      },
      {
        "Endpoint": "post:/api/players/*/items/*",
        "Period": "3s",
        "Limit": 1
      },
      {
        "Endpoint": "get:/api/players/*",
        "Period": "3s",
        "Limit": 1
      },
      {
        "Endpoint": "put:/api/players/*",
        "Period": "3s",
        "Limit": 1
      },
      {
        "Endpoint": "delete:/api/players/*",
        "Period": "3s",
        "Limit": 1
      },
      {
        "Endpoint": "get:/api/players",
        "Period": "3s",
        "Limit": 1
      },
      {
        "Endpoint": "put:/api/players",
        "Period": "3s",
        "Limit": 1
      },
      {
        "Endpoint": "delete:/api/players",
        "Period": "3s",
        "Limit": 1
      },
      {
        "Endpoint": "*:/api/items/*",
        "Period": "3s",
        "Limit": 1
      },
      {
        "Endpoint": "*:/api/users/*",
        "Period": "3s",
        "Limit": 1
      },
      {
        "Endpoint": "*:/api/monsters/*",
        "Period": "3s",
        "Limit": 1
      },
      {
        "Endpoint": "*:/api/items",
        "Period": "3s",
        "Limit": 1
      },
      {
        "Endpoint": "*:/api/users",
        "Period": "3s",
        "Limit": 1
      },
      {
        "Endpoint": "*:/api/monsters",
        "Period": "3s",
        "Limit": 1
      }
    ]
  }
}
